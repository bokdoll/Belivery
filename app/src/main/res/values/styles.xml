<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/white</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>

    <style name="MainTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/colorMint</item>
    </style>

    <style name="customRatingBar_dialog_delivery_end" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_dialog_delivery_end</item>
    </style>

    <style name="RestaurantTabLayoutTextStyle">
        <item name="android:textSize">13sp</item>
        <item name="android:lineSpacingExtra"> 20sp</item>
    </style>

</resources>
